<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_FlowForm" name="com.bjsxt.oa.model.FlowForm">
    <id name="id">
      <generator class="native"/>
    </id>
    <many-to-one name="workflow"/>
    <property name="template"/>
    <set inverse="true" name="fields" order-by="id">
    	<key column="flowFormId"></key>
    	<one-to-many class="com.bjsxt.oa.model.FormField"></one-to-many>
    </set>
  </class>
</hibernate-mapping>
